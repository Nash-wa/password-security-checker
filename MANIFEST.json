{
  "manifest_version": 3,
  "name": "password checker extension",
  "version": "1.0",
  "description": "check the secuirity of password",
  "browser_action": {
    
    "default_icon": {
      
    }
  },
  "permissions": [
    "activeTab"
  ],
  
  "background": {
    "scripts": ["extension.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["extension.js"]
    }
  ]
}
